<script type="text/template" id="tmplBetListHeader">
    <dl>
        <dd class="search">
            <div class="ui-widget">
                <form>
                    <i class="ni ni-search"></i>
                    <input class="form-control typeahead" id="txtbSearch" type="text" placeholder="Maç Kodu / Maç Adı Ara">
                </form>
            </div>
            <div class="ac_results" style="display: none; position: absolute; width: 285px !important; top: 265px !important; left: 457px !important;">
            </div>
        </dd>
        <dd class="mbs" data-text="MBS" data-value="{{Model.SelectedOCGroups[0]}}">
            <span>MBS</span>
            <small data-value="mbs">
                
                {% if(Model.CurrentEventType === "18") { %}
                &nbsp;
                {% } else { %}
                <i class="ni ni-caret-up sort" onclick=" BetListUI.SortBultenEvents(this); "></i><i class="ni ni-caret-down sort" onclick="BetListUI.SortBultenEvents(this); "></i>
                {% } %}
            </small>
        </dd>
        {%
        _.each(Model.SelectedOCGroups,function(item,key,list){

        var currentOCG= BetListClient.GetOCG(item);
        if(!_.isUndefined(currentOCG)) {

        var handicapClass = currentOCG.IsHandicap ? 'hc' : ''
        %}
        
        {%if(item == "3" && !(BetListOptions.IsProbable && BetListOptions.CurrentEventType == BetListConstants.EventTypes.FootballDuel)){%}
        <dd class="mbs" data-text="MBS" data-value="3">
            <span>MBS</span>
            <small data-value="mbs">
                <i class="ni ni-caret-up sort" onclick=" BetListUI.SortBultenEvents(this); "></i><i class="ni ni-caret-down sort" onclick="BetListUI.SortBultenEvents(this); "></i>
            </small>
        </dd>
        {%} %}

        <dd class="oc{{Model.CurrentEventType}}-{{item}} ocg{{currentOCG.Value.length}} {{handicapClass}}" data-text="{{currentOCG.Title}}" data-value="{{item}}">
            <span>{{currentOCG.Title}}</span>
            {%

            _.each(currentOCG.Value, function(ocg) {

            var flag = BetListViewHelper.ShouldDisplaySort(Model.CurrentEventType, item, ocg.id);

            if(!ocg.ishandicap) {

            if(flag){

            %}
            <small data-value="{{ocg.id}}"></small>
            {%}else{%}
            <small data-value="{{ocg.id}}">
                <i class="ni ni-caret-up sort" onclick=" BetListUI.SortBultenEvents(this); "></i>
                <i class="ni ni-caret-down sort" onclick=" BetListUI.SortBultenEvents(this); "></i>
            </small>
            {%}%}

            {%}%}
            {% if(ocg.ishandicap){ %}
            <small class="hc"> &nbsp; </small>
            {%}%}
            {%});%}
        </dd>
        {%}});%}

        {%
        if(BetListOptions.IsDuel && !BetListOptions.IsProbable) {
        %}
        <dd class="other toggleDuelDetails">
            <div>
                <i class="ni ni-arrow-circle-o-down"></i>
                <i class="ni ni-arrow-circle-o-up"></i>
            </div>
        </dd>
        {% } else { %}
        <dd class="other" data-text="+">&nbsp;</dd>
        {% }%}
    </dl>
</script>

<script type="text/template" id="tmplBetListRow">
    {%
    _.each(Model.PagedList(),function(leagueEvents,key,list){
    var LEModel = Model.LEWrapper ? Model.LEWrapper.GetModel(leagueEvents) : {};
    %}
    {%if(LEModel.HasNextBets){%}
    <div class="NextBets"> İLERİ TARİHLİ MAÇLAR </div>
    {%}%}
    {%if(LEModel.HasSubHeader){%}
    <dl class="sub-header">
        <dd class="lname">{{LEModel.Date}} {{LEModel.Day}} <b>-</b> <span>{{LEModel.LeagueName}}</span></dd>
        <dd class="mbs" data-text="MBS">&nbsp;</dd>
        {%
        var mbsDisplayed = false;
        _.each(Model.DisplayGroups,function(OCGroupId,key,list){
        var currentOCG= BetListClient.GetOCG(OCGroupId);
        if(!_.isUndefined(currentOCG)){
        _.each(currentOCG.Value ,function(oc,key,list){
        %}

        {%
        if(!mbsDisplayed && OCGroupId == "3") {
        mbsDisplayed = true;
        %}

        <dd class="mbs" data-text="MBS">&nbsp;</dd>
        {% } %}
        <dd class="oc{{Model.CurrentEventType}}-{{OCGroupId}}" data-text="{{oc.title}}">{{oc.title}}</dd>
        {% });}}); %}
        <dd class="other" data-text="+">+</dd>
    </dl>
    {%}%}
    {%
    _.each(leagueEvents,function(eventItem,key,list){
    var RowModel=BetListViewHelper.GetBetListRowModel(eventItem);
    %}
    <dl id="r_{{eventItem.C}}" class="odds-row {{RowModel.BombClass}}" data-value="{{eventItem.C}}" data-commentcount="{{eventItem.CC}}" data-percentage="{{eventItem.P}}" showplayratio="false">
        {% if(RowModel.IsBombOfTheDay) { %}
        <dd class="bomb" data-toggle="tooltip" data-placement="top" data-original-title="Günün Fırsatı"><a href="/yardim/Oyunlar/255/Iddaa#Gunun-Firsati-Nedir" target="_blank" rel="noopener"><i class="gunun-firsati"></i></a></dd>
        {% } %}
        <dd class="code">{{eventItem.C}}</dd>
        <dd class="date">{{eventItem.T}}</dd>
        <dd class="name">
            {% if(!BetListOptions.IsProbable) { %}
            <a href="{{BetListClient.GetStatisticsURL(eventItem.C)}}" target="_blank">{{RowModel.EventName}}</a>
            {% }else{ %}
            <a style="cursor: default;">{{RowModel.EventName}}</a>
            {% } %}
        </dd>

        <dd class="cast">
            {% if(RowModel.BroadcastClass != ''){ %}
            <span class="{{RowModel.BroadcastClass}}" data-toggle="tooltip" data-placement="top" data-original-title="{{RowModel.BroadCastToolTip}}" data-content="{{RowModel.BroadCastToolTip}}" ></span>
            {% } %}  

            {% if(!BetListOptions.IsProbable && 
            (BetListOptions.CurrentEventType == BetListConstants.EventTypes.Football || 
            BetListOptions.CurrentEventType == BetListConstants.EventTypes.Basketball || 
            BetListOptions.CurrentEventType == BetListConstants.EventTypes.BasketballDuel || 
            BetListOptions.CurrentEventType == BetListConstants.EventTypes.FootballDuel ||
            BetListOptions.CurrentEventType == BetListConstants.EventTypes.FootballGold)) { %}         
            <span class="ni ni-sm ni-comment" data-toggle="tooltip" data-placement="top" onclick="BetNameToolTip.ShowMemberCommentPopup({{eventItem.C}}); " title="Yorumlar"></span>      
            {% } %}        
        </dd>
        <dd class="mWrap">&nbsp;</dd>
        {% var _mbs = BetListViewHelper.GetOutcomeGroupMbs(Model.DisplayGroups, eventItem.OCG);
        if(_mbs != undefined) {
        %}
        <dd class="mbs" data-text="MBS"><span class="mbs{{_mbs}}">{{_mbs}}</span></dd>
        {%}%}
        {%
        var flag = false;
        _.each(Model.DisplayGroups,function(OCGroupId,key,list){
        var currentOCG= BetListClient.GetOCG(OCGroupId);
        if(!_.isUndefined(currentOCG)){

        var dataText = '';

        _.each(currentOCG.Value,function(oc,key,list){
        var OCModel = BetListViewHelper.GetOCModel(OCGroupId,eventItem.C,oc);       
        dataText = (BetListOptions.IsMobile && currentOCG.Prefix != undefined) ? currentOCG.Prefix + oc.title : oc.title;
        %}

        {% if(!flag && OCGroupId == "3"){
        flag = true;
        var _mbs = '';
        if(eventItem.OCG[3] != undefined && eventItem.OCG[3] != undefined) {
        _mbs = eventItem.OCG[3].MBS;
        }else {
        _mbs = '';
        }
        %}
        <dd class="mbs" data-text="MBS"><span class="mbs{{_mbs}}">{{_mbs}}</span></dd>
        {%}%}

        {%if(parseInt(OCModel.OddValue) > 0 && parseInt(oc.id) >= 0 && !BetListOptions.IsProbable) {%}
        <dd id="{{eventItem.C}}_{{oc.id}}" class="oc{{Model.CurrentEventType}}-{{OCGroupId}} {{OCModel.ExtendedClass}}" data-bwpc="{{OCModel.BWPC}}" data-value="{{oc.id}}" data-text="{{dataText}}"
            onclick='BetListClient.AddEvent("{{eventItem.C}}", "{{oc.id}}", "{{OCModel.Mbs}}", "{{OCModel.OddValue}}", "{{OCGroupId}}", this);'>
            <div>
                {% if(OCModel.HasOddChange){ %}
                <i class="{{OCModel.ChangeClass}}"></i>
                {%}%}
                <span class="odd">{{OCModel.OddValue}}</span>
                {%if(OCModel.Percentage != '' || OCModel.Percentage != '%0'){ %}
                <small>{{OCModel.Percentage}}</small>
                {%}%}
            </div>
        </dd>
        {%} else if (parseInt(OCModel.OddValue) > 0 && parseInt(oc.id) >= 0 && BetListOptions.IsProbable){%}
        <dd id="{{eventItem.C}}_{{oc.id}}" class="oc{{Model.CurrentEventType}}-{{OCGroupId}} {{OCModel.ExtendedClass}}" data-bwpc="{{OCModel.BWPC}}" data-value="{{oc.id}}" data-text="{{dataText}}" style="cursor: default;"
            onclick="BetListClient.ProbableWarning();">
            <div>
                <span class="odd">{{OCModel.OddValue}}</span>
                {%if(OCModel.Percentage != '' || OCModel.Percentage != '%0'){ %}
                <small>{{OCModel.Percentage}}</small>
                {%}%}
            </div>
        </dd>
        {%}else{%}
        <dd id="{{eventItem.C}}_{{oc.id}}" class="oc{{Model.CurrentEventType}}-{{OCGroupId}} {{OCModel.ExtendedClass}}" style="cursor: default;" data-bwpc="{{OCModel.BWPC}}" data-value="{{oc.id}}" data-text="{{dataText}}">
            <div>
                {% if(OCModel.HasOddChange){ %}
                <i class="{{OCModel.ChangeClass}}"></i>
                {% } %}
                <span class="odd">{{OCModel.OddValue}}</span>
            </div>
        </dd>
        {%}});}%}

        {%});%}

        {% if(RowModel.OtherInfo.Count > 0){ %}
        <dd id="o_{{eventItem.C}}" class="more m-odd {{RowModel.OtherInfo.BWPClass}}" data-bwpc="{{RowModel.OtherInfo.BWPC}}" data-bwpc-oc="{{RowModel.OtherInfo.BWPOutcome}}" data-value="{{eventItem.C}}" data-text="+" showplayratio="false">
            {% if(BetListOptions.IsMacSkoru != true) { %}
            <div>
                <span>{{RowModel.OtherInfo.Count}}</span>
                <i class="ni ni-caret-up"></i>
                <i class="ni ni-caret-down"></i>
            </div>
            {% } %}           
        </dd>
        {%}else{%}
        <dd id="o_{{eventItem.C}}" class="more m-odd" style="cursor: default;" data-value="" data-text="+" showplayratio="false">
            <div>
            </div>
        </dd>
        {%}%}

        {%if(!BetListOptions.IsProbable){ %}
        <dd class="mOther" data-value="{{eventItem.C}}" data-text=""> <i class="ni ni-t-arrow-left"></i> </dd>
        {%} else { %}
        <dd class="mOther passive" data-value="{{eventItem.C}}" data-text=""> <i class="ni ni-t-arrow-left"></i> </dd>
        {% } %}

    </dl>
    <div id="oo_{{eventItem.C}}" class="other-odds" data-value="{{eventItem.C}}" style="display: none;" showplayratio="false"></div>
    {%});%}

    {%});%}

</script>

