                {%if(OCModel.Percentage != '' || OCModel.Percentage != '%0'){ %}
                <small>{{OCModel.Percentage}}</small>
                {%}%}
            </div>
        </dd>
        {%}else if(BetListOptions.IsProbable){%}
        <dd id="{{eventItem.C}}_{{oc.id}}" class="oc{{Model.CurrentEventType}}-{{OCGroupId}} {{OCModel.ExtendedClass}}" style="cursor: default;" data-bwpc="{{OCModel.BWPC}}" data-value="{{oc.id}}" data-text="{{oc.title}}"
