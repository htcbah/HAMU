<script type="text/template" id="tmplOtherBets">
    {%
    var displaySwitch = '';
    _.each(Model.OCGroups,function(OCGroupId,skey,slist){

    if(BetListOptions.IsMacSkoru) {
    if((OCGroupId == '4_1' || OCGroupId == '4_X' || OCGroupId == '4_2')) {
    displaySwitch = '';
    } else {
    displaySwitch = 'display: none;';
    }
    }
    var OCGModel=BetListViewHelper.GetOCGModel(Model.Event,OCGroupId,Model.IsSpecial);
    if(!_.isUndefined(OCGModel.CurrentOCG)){
    %}
    <dl class="odds-row {{Model.SpecialEventClass}}" style="{{displaySwitch}}">
        <dd class="bet-name">{{OCGModel.Name}}</dd>
        <dd class="mWrap">&nbsp;</dd>
