                {%if(OCModel.Percentage != '' || OCModel.Percentage != '%0'){ %}
                <small>{{OCModel.Percentage}}</small>
                {%}%}
            </div>
        </dd>
        {% } else if (_.isNaN(parseInt(OCModel.OddValue)) > 0 && BetListOptions.IsProbable){%}
        <dd id="{{eventItem.C}}_{{oc.id}}" class="oc{{Model.CurrentEventType}}-{{OCGroupId}} {{OCModel.ExtendedClass}}" data-bwpc="{{OCModel.BWPC}}" data-value="{{oc.id}}" data-text="{{oc.title}}" style="cursor: default;">
            <div>
