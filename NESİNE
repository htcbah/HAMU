            <span class="odd-title">MBS</span>
            <div>
                <span class="mbs{{OCGModel.Mbs}}">{{OCGModel.Mbs}}</span>
            </div>
        </dd>
        {%      
        _.each(OCGModel.CurrentOCG.Value,function(oc,okey,olist){

        var OCModel = BetListViewHelper.GetOCModel(OCGroupId,Model.Event.C,oc);
        %}

        {% if (OCModel.OddValue != "-" && !OCModel.IsHandicap  && !BetListOptions.IsProbable) { %}
        <dd id="{{Model.Event.C}}_{{oc.id}}" class="oc{{Model.Event.TYPE}}-{{OCGroupId}} {{OCModel.ExtendedClass}}" data-bwpc="{{OCModel.BWPC}}" data-value="{{oc.id}}" data-text="{{OCModel.Name}}"
            onclick='BetListClient.AddEvent("{{Model.Event.C}}", "{{oc.id}}", "{{OCModel.Mbs}}", "{{OCModel.OddValue}}", "{{OCGroupId}}", this);' >
            {% if(Model.IsSpecial) { %}
            <div>
                <span class="odd-title">{{OCModel.Name}}</span>
                <small>{{OCModel.Percentage}}</small>
                <span class="odd">{{OCModel.OddValue}}</span>
                {%
                } else {
                %}
                <span class="odd-title">{{OCModel.Name}}</span>
                <div>
                    {% if(OCModel.HasOddChange){ %}
                    <i class="{{OCModel.ChangeClass}}"></i>
                    {%}%}
                    <span class="odd">{{OCModel.OddValue}}</span>
                    <small>{{OCModel.Percentage}}</small>
                    {%
                    }
                    %}
                </div>
        </dd>
        {%} else if (parseInt(OCModel.OddValue) > 0 && parseInt(oc.id) >= 0 && BetListOptions.IsProbable){%}
        <dd id="{{Model.Event.C}}_{{oc.id}}" class="oc{{Model.Event.TYPE}}-{{OCGroupId}} {{OCModel.ExtendedClass}}" data-bwpc="{{OCModel.BWPC}}" data-value="{{oc.id}}" data-text="{{OCModel.Name}}" style="cursor: default;"
            onclick="BetListClient.ProbableWarning();">
            <span class="odd-title">{{OCModel.Name}}</span>
            <div>
                {% if(OCModel.HasOddChange){ %}
                <i class="{{OCModel.ChangeClass}}"></i>
                {%}%}
                <span class="odd">{{OCModel.OddValue}}</span>
            </div>
        </dd>
        {% } else {%}
        <dd id="{{Model.Event.C}}_{{oc.id}}" class="oc{{Model.Event.TYPE}}-{{OCGroupId}} {{OCModel.ExtendedClass}}" style="cursor: default;" data-bwpc="{{OCModel.BWPC}}" data-value="{{oc.id}}" data-text="{{OCModel.Name}}">
            {%
            if(Model.IsSpecial) {
            %}
            <div>
                <span class="odd-title">{{OCModel.Name}}</span>
                <small>{{OCModel.Percentage}}</small>
                {% if(OCModel.HasOddChange){ %}
                <i class="{{OCModel.ChangeClass}}"></i>
                {%}%}
                <span class="odd">{{OCModel.OddValue}}</span>
                {%
                } else {
                %}
                <span class="odd-title">{{OCModel.Name}}</span>
                <div>
                    <span class="odd">{{OCModel.OddValue}}</span>
                    {% if(OCModel.HasOddChange){ %}
                    <i class="{{OCModel.ChangeClass}}"></i>
                    {%}%}
                    <small>{{OCModel.Percentage}}</small>
                    {% } %}
                </div>
        </dd>
        {% } %}

        {%}); %}
    </dl>
    {% }
    });
