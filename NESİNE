<script type="text/template" id="tmplLeagueFilter">
    {%
    var size = _.size(Model);

    var N = parseInt( (size / 10) + 1, 10);
    var loop = _.range(N);

    _.each(loop, function(index) {
    var items = _.chain(Model).rest((index*10)).take(10).value();
    %}
    <div class="rows col-sm-12">
        {%
        _.each(items,function(item,key,list){
        %}
        <div class="col-sm-6 no-padding-r">
            <label>
              <input type="checkbox" data-value="{{item.Code}}" />
              <i class="flag-{{item.FlagCode}}"></i> <span>{{item.Name}}</span> <b>({{ item.Capacity }})</b>
            </label>
        </div>
        {%
        });
        %}
    </div>
    {%
    });
    %}
</script>

